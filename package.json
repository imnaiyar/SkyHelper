{
  "name": "skyhelper-workspace",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "build:affected": "turbo build --filter=...[origin/main]",
    "bot:dev": "pnpm --filter skyhelper dev",
    "jobs:dev": "pnpm --filter jobs dev",
    "bot": "pnpm --filter skyhelper start",
    "lint": "turbo lint --filter=!@skyhelperbot/website",
    "lint:affected": "turbo lint --filter=...[origin/main]",
    "test:affected": "turbo test --filter=...[origin/main]",
    "test": "turbo test",
    "update": "pnpm --recursive update --interactive",
    "prepare": "husky",
    "check-locales": "node scripts/check-unused-locales.js",
    "clean-locales": "node scripts/check-unused-locales.js --remove"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@types/node": "^22.13.1",
    "eslint": "9.36.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "globals": "^16.4.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.44.0"
  },
  "packageManager": "pnpm@10.15.1",
  "pnpm": {
    "overrides": {
      "discord-api-types": "latest"
    },
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@scarf/scarf",
      "@sentry/cli",
      "@sentry/profiling-node",
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@octokit/action": "^7.0.0"
  }
}
