{
  "openapi": "3.0.0",
  "paths": {
    "/": {
      "get": {
        "description": "Returns a simple hello world message to verify the API is running",
        "operationId": "AppController_getHello",
        "parameters": [],
        "responses": {
          "200": {
            "description": "API is running successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "example": "Hello World!"
                }
              }
            }
          }
        },
        "summary": "Health check endpoint",
        "tags": ["Application"]
      }
    },
    "/shards-embed": {
      "get": {
        "description": "Retrieves Discord embed data for shard information on a specific date",
        "operationId": "AppController_getShardsEmbed",
        "parameters": [
          {
            "name": "locale",
            "required": false,
            "in": "query",
            "description": "Language locale",
            "schema": {
              "example": "en-US",
              "type": "string"
            }
          },
          {
            "name": "user",
            "required": false,
            "in": "query",
            "description": "User ID for personalized content",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "noBtn",
            "required": false,
            "in": "query",
            "description": "Exclude buttons from embed",
            "schema": {
              "enum": ["true", "false"],
              "type": "string"
            }
          },
          {
            "name": "date",
            "required": false,
            "in": "query",
            "description": "Date in YYYY-MM-DD format",
            "schema": {
              "example": "2024-01-15",
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Discord embed object for shard information",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "title": {
                      "type": "string"
                    },
                    "description": {
                      "type": "string"
                    },
                    "color": {
                      "type": "number"
                    },
                    "timestamp": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "summary": "Get shards embed data",
        "tags": ["Application"]
      }
    },
