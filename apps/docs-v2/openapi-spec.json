{
  "openapi": "3.0.0",
  "paths": {
    "/": {
      "get": {
        "operationId": "getHello",
        "parameters": [],
        "responses": {
          "200": {
            "description": ""
          }
        },
        "tags": ["App"]
      }
    },
    "/shards-embed": {
      "get": {
        "operationId": "getShardsEmbed",
        "parameters": [
          {
            "description": "The date for which to get the shards, in the format 'YYYY-MM-DD'",
            "required": false,
            "name": "date",
            "in": "query",
            "schema": {
              "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
              "type": "string"
            }
          },
          {
            "required": false,
            "name": "noBtn",
            "in": "query",
            "schema": {
              "allOf": [
                {
                  "$ref": "#/components/schemas/noBtn"
                }
              ]
            }
          },
          {
            "description": "",
            "required": false,
            "name": "user",
            "in": "query",
            "schema": {
              "pattern": "^(?<id>\\d{17,20})$",
              "type": "string"
            }
          },
          {
            "required": false,
            "name": "locale",
            "in": "query",
            "schema": {
              "allOf": [
                {
                  "$ref": "#/components/schemas/locale"
                }
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }
        },
        "tags": ["App"]
      }
    },
    "/times-embed": {
      "get": {
        "operationId": "getTimesEmbed",
