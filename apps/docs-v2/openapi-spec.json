              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": ""
          }
        },
        "tags": ["App"]
      }
    },
    "/guilds/{guild}": {
      "get": {
        "operationId": "getGuild",
        "parameters": [
          {
            "name": "guild",
            "required": true,
            "in": "path",
            "description": "The ID of the guild",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "The basic guild info",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GuildInfoDto"
                }
              }
            }
          }
        },
        "security": [
          {
            "auth-token": []
          }
        ],
        "tags": ["Guild"]
      },
      "patch": {
        "operationId": "updateGuild",
        "parameters": [
          {
            "name": "guild",
            "required": true,
            "in": "path",
            "description": "The ID of the guild",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "description": "The updated guild info",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/GuildInfoDto"
              }
            }
          }
        },
        "responses": {
          "default": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/GuildInfoDto",
                      "description": "The update response"
                    },
                    {
                      "type": "string",
                      "example": "null",
                      "description": "Guild not found"
                    }
                  ]
                }
              }
            }
          }
        },
        "security": [
          {
            "auth-token": []
          }
        ],
        "tags": ["Guild"]
      }
    },
    "/guilds/{guild}/features/{feature}": {
      "get": {
        "operationId": "getFeature",
        "parameters": [
          {
            "name": "feature",
            "required": true,
            "in": "path",
            "schema": {
              "enum": ["live-updates", "reminders"],
              "type": "string"
            }
          },
          {
            "name": "guild",
            "required": true,
            "in": "path",
            "description": "The ID of the guild",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns the feature data if enabled, otherwise returns null or undefined",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object"
                }
              }
            }
          }
        },
        "security": [
          {
            "auth-token": []
          }
        ],
        "tags": ["Guild"]
      },
      "post": {
        "operationId": "enableFeature",
        "parameters": [
          {
            "name": "feature",
            "required": true,
            "in": "path",
            "schema": {
              "enum": ["live-updates", "reminders"],
              "type": "string"
            }
          },
          {
            "name": "guild",
            "required": true,
            "in": "path",
            "description": "The ID of the guild",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "type": "string",
                      "example": "Success"
                    },
                    {
                      "type": "string",
                      "example": "null"
                    }
                  ]
                }
              }
            }
          }
        },
        "security": [
          {
            "auth-token": []
          }
        ],
        "tags": ["Guild"]
      },
      "patch": {
        "operationId": "updateFeature",
        "parameters": [
          {
            "name": "guild",
            "required": true,
            "in": "path",
            "description": "The ID of the guild",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "feature",
            "required": true,
            "in": "path",
            "schema": {
              "enum": ["live-updates", "reminders"],
              "type": "string"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "Returns the updated feature data or null if not found",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "$ref": "#/components/schemas/ReminderFeatureDto"
                    },
                    {
                      "type": "object",
                      "properties": {
                        "times": {
                          "type": "string",
                          "nullable": true
                        },
                        "shards": {
                          "type": "string",
                          "nullable": true
                        }
                      }
                    },
                    {
                      "type": "string",
                      "example": "null"
                    }
                  ]
                }
              }
            }
          }
        },
        "security": [
          {
            "auth-token": []
          }
        ],
        "tags": ["Guild"]
      },
      "delete": {
        "operationId": "disableFeature",
        "parameters": [
          {
            "name": "feature",
            "required": true,
            "in": "path",
            "schema": {
              "enum": ["live-updates", "reminders"],
              "type": "string"
            }
          },
          {
            "name": "guild",
            "required": true,
            "in": "path",
            "description": "The ID of the guild",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "Returns success status or null if feature deletion failed",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "type": "string",
                      "example": "Success"
                    },
                    {
                      "type": "string",
                      "example": "null"
                    }
                  ]
                }
              }
            }
          }
        },
        "security": [
          {
            "auth-token": []
          }
        ],
        "tags": ["Guild"]
      }
    },
    "/guilds/{guild}/channels": {
      "get": {
        "operationId": "getChannels",
        "parameters": [
          {
            "name": "guild",
