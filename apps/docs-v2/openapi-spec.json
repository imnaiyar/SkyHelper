  "info": {
    "title": "SkyHelper API",
    "description": "API Documentation for SkyHelper",
    "version": "1.0",
    "contact": {}
  },
  "tags": [
    {
      "name": "skyhelper",
      "description": ""
    }
  ],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "noBtn": {
        "type": "string",
        "enum": ["true", "false"]
      },
      "locale": {
        "type": "string",
        "enum": ["en-US", "hi", "ru", "ja"]
      },
      "GuildInfoDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": ""
          },
          "name": {
            "type": "string",
            "description": ""
          },
          "icon": {
            "type": "string",
            "nullable": true
          },
          "prefix": {
            "type": "string",
            "description": ""
          },
          "announcement_channel": {
            "type": "string",
            "description": ""
          },
          "beta": {
            "type": "boolean"
          },
          "language": {
            "type": "string",
            "description": ""
          },
          "enabledFeatures": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": ["reminders", "live-updates"]
            }
          }
        }
      },
      "TSDataDto": {
        "type": "object",
        "properties": {
          "spirit": {
            "type": "string",
            "description": ""
          },
          "visitDate": {
            "type": "string",
            "description": "",
            "pattern": "^\\d{2}-\\d{2}-\\d{4}$"
          },
          "index": {
            "type": "string",
            "description": ""
          }
        },
        "required": ["spirit", "visitDate", "index"]
      },
      "EventDataDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": ""
          },
          "startDate": {
            "type": "string",
            "description": "",
            "pattern": "^\\d{2}-\\d{2}-\\d{4}$"
          },
          "endDate": {
            "type": "string",
            "description": "",
            "pattern": "^\\d{2}-\\d{2}-\\d{4}$"
          }
        },
        "required": ["name", "startDate", "endDate"]
      },
      "DynamicDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": ""
          },
          "date": {
            "type": "string",
            "description": ""
          },
          "description": {
            "type": "string",
            "description": ""
          },
          "images": {
            "items": {
              "type": "array"
            },
            "type": "array"
          }
        },
        "required": ["title", "date", "images"]
      },
      "QuestsDTO": {
        "type": "object",
        "properties": {
          "quests": {
            "items": {
              "type": "array"
            },
            "type": "array"
          },
          "last_updated": {
            "type": "string",
            "description": ""
          },
          "last_message": {
            "type": "string",
            "description": ""
          },
          "rotating_candles": {
            "$ref": "#/components/schemas/DynamicDto"
          },
          "seasonal_candles": {
            "$ref": "#/components/schemas/DynamicDto"
          }
        },
        "required": ["quests", "last_updated", "rotating_candles"]
      }
    }
  }
}
