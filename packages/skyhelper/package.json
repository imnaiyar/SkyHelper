{
  "name": "skyhelper",
  "version": "7.1.1",
  "description": "A Bot for Sky: Children of the Light",
  "main": "dist/bot/index.js",
  "type": "module",
  "keywords": [
    "SkyCoTL",
    "SkyShards",
    "SkyDiscordBot",
    "Sky: Children of the Light"
  ],
  "scripts": {
    "commands:dev": "tsx --env-file=.env src/bot/handlers/registerCommands.ts",
    "start": "npm run build:prod && node dist/bot/index.js",
    "dev": "tsx --env-file=.env src/bot/index.js",
    "build:prod": "tsc -p tsconfig.json && npm run sentry:sourcemaps",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --fix src",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules --import ./scripts/register-loader.mjs\" jest",
    "commands": "node --env-file=.env dist/bot/handlers/registerCommands.js",
    "role-metadata": "node --env-file=.env dist/bot/commands/roleConnectionMetadata",
    "format": "prettier --write --ignore-unknown src",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org skyhelper-09 --project node ./dist && sentry-cli sourcemaps upload --org skyhelper-09 --project node ./dist",
    "setup": "node scripts/env-cli.js"
  },
  "author": "Naiyar Imam",
  "license": "MIT",
  "dependencies": {
    "@discordjs/collection": "^2.1.1",
    "@discordjs/core": "^2.0.1",
    "@discordjs/rest": "^2.3.0",
    "@discordjs/ws": "^2.0.1",
    "@imnaiyar/framework": "^1.2.3",
    "@napi-rs/canvas": "^0.1.65",
    "@nestjs/common": "^11.0.6",
    "@nestjs/core": "^11.0.6",
    "@nestjs/platform-express": "^11.0.6",
    "@sapphire/discord-utilities": "^3.4.4",
    "@sapphire/type": "^2.4.4",
    "@sentry/cli": "^2.32.2",
    "@sentry/node": "^8.34.0",
    "@sentry/profiling-node": "^8.17.0",
    "@skyhelperbot/constants": "workspace:^",
    "@skyhelperbot/utils": "workspace:^",
    "chalk": "^4.1.2",
    "cropify": "^1.0.9",
    "express": "^4.20.0",
    "i18next": "^24.2.1",
    "i18next-fs-backend": "^2.3.1",
    "luxon": "^3.5.0",
    "mongoose": "^8.9.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/testing": "^11.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.10.10",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.4"
  },
  "packageManager": "pnpm@9.4.0",
  "pnpm": {
    "overrides": {
      "discord-api-type": "0.37.97"
    }
  }
}
