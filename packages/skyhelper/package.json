{
  "name": "skyhelper",
  "version": "7.7.0",
  "description": "A Bot for Sky: Children of the Light",
  "main": "dist/bot/index.js",
  "type": "module",
  "files": [
    "dist/**/*",
    "scripts/**/*",
    "src/**/*",
    "tsconfig.json"
  ],
  "keywords": [
    "SkyCoTL",
    "SkyShards",
    "SkyDiscordBot",
    "Sky: Children of the Light"
  ],
  "scripts": {
    "commands:dev": "tsx --env-file=.env src/bot/handlers/registerCommands.ts",
    "start": "node dist/bot/index.js",
    "dev": "tsx --env-file=.env src/bot/index.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --fix src",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules --import ./scripts/register-loader.mjs\" jest",
    "commands": "node dist/bot/handlers/registerCommands.js",
    "role-metadata": "node --env-file=.env dist/bot/commands/roleConnectionMetadata",
    "format": "prettier --write --ignore-unknown src",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org skyhelper-09 --project node ./dist && sentry-cli sourcemaps upload --org skyhelper-09 --project node ./dist",
    "setup": "node scripts/env-cli.js",
    "generate:openapi": "tsx scripts/generate-openapi.ts"
  },
  "author": "Naiyar Imam",
  "license": "MIT",
  "dependencies": {
    "@discordjs/collection": "^2.1.1",
    "@discordjs/core": "3.0.0-dev.1739059974-af3ab2211",
    "@discordjs/rest": "^2.3.0",
    "@discordjs/ws": "^2.0.1",
    "@imnaiyar/framework": "latest",
    "@napi-rs/canvas": "^0.1.65",
    "@nestjs/common": "^11.0.6",
    "@nestjs/core": "^11.0.6",
    "@nestjs/platform-express": "^11.0.6",
    "@nestjs/swagger": "^11.2.0",
    "@sapphire/discord-utilities": "^3.4.4",
    "@sapphire/string-store": "^2.0.0",
    "@sentry/cli": "^2.37.0",
    "@sentry/nestjs": "^8.54.0",
    "@sentry/node": "^8.49.0",
    "@sentry/profiling-node": "^8.49.0",
    "@skyhelperbot/constants": "workspace:^",
    "@skyhelperbot/utils": "workspace:^",
    "chalk": "^4.1.2",
    "cropify": "^1.0.9",
    "discord-api-types": "0.38.10",
    "express": "^4.20.0",
    "i18next": "^24.2.1",
    "i18next-fs-backend": "^2.3.1",
    "luxon": "^3.5.0",
    "mongoose": "^8.9.5",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@nestjs/testing": "^11.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.13.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.1.4",
    "tsx": "^4.20.4",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@9.4.0"
}
